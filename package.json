{
  "name": "gochu",
  "version": "0.0.0-development",
  "description": "Create DOM elements easily as testable components.",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "webpack -p --bail",
    "pretest": "npm run build",
    "test": "eslint src/ test/ && ava",
    "tdd": "ava -w",
    "preexample": "npm run build",
    "example": "cd example/ && webpack && http-server dist/",
    "prerelease": "npm run build",
    "release": "semantic-release pre && npm publish && semantic-release post",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bbmoz/gochu.git"
  },
  "keywords": [
    "modules",
    "tree",
    "deps",
    "dependencies",
    "graph",
    "chart",
    "import",
    "export",
    "require",
    "ast",
    "es6"
  ],
  "author": "Joe Lee <https://github.com/bbmoz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbmoz/gochu/issues"
  },
  "homepage": "https://github.com/bbmoz/gochu",
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "failWithoutAssertions": true,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-latest": "^6.24.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "esprima": "^3.1.3",
    "husky": "^0.13.3",
    "semantic-release": "^6.3.2",
    "sinon": "^2.1.0",
    "validate-commit-msg": "^2.12.1",
    "webpack": "^2.3.2"
  }
}
